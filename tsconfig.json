{
  "compilerOptions": {
    "declaration": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": ["es2016", "es6"],
    "moduleResolution": "node",
    "strict": true,
    "target": "esnext",
    "sourceMap": true,
    "outDir": "dist",
    "rootDir": "src",
    "paths": {
      "@expo-image-cropper": ["./src/index"]
    }
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
